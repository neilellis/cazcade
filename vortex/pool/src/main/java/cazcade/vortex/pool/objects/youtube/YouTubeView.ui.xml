<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:image="urn:import:cazcade.vortex.widgets.client.image">
    <g:HTMLPanel styleName="pool-object-youtube">
        <div class="pool-object-youtube-inner">
            <div class="pool-object-youtube-preview">
                <image:CachedImage ui:field="image" visible="false" size="CLIPPED_MEDIUM"/>
            </div>

            <g:HTMLPanel ui:field="videoFrameHolder" styleName="pool-object-youtube-video">
                <iframe ui:field="videoFrame" width="100%" height="100%" style="border:none;display:block" src=""
                        frameborder="0"
                        allowfullscreen="true"></iframe>
            </g:HTMLPanel>

        </div>

    </g:HTMLPanel>

</ui:UiBinder>