<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:image="urn:import:cazcade.vortex.widgets.client.image">
    <g:HTMLPanel styleName="pool-object-inner">

        <g:HTMLPanel styleName="pool-object-photo">
            <g:AbsolutePanel>
                <g:HTMLPanel styleName="pool-object-photo-surround" ui:field="imageSurround">
                    <image:CachedImage ui:field="image" size="CLIPPED_MEDIUM"/>
                </g:HTMLPanel>
                <g:Image styleName="photo-effect" ui:field="effect" url="/_static/_images/photo-masks/gloss-medium-noframe.png"/>
            </g:AbsolutePanel>


        </g:HTMLPanel>
    </g:HTMLPanel>

</ui:UiBinder>